<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/back2h"
    tools:context=".CategoryView">

    <TextView
        android:id="@+id/textView"
        android:layout_width="258dp"
        android:layout_height="39dp"
        android:layout_marginBottom="8dp"
        android:gravity="center"
        android:text="@string/add_new_product"
        android:textColor="@color/azur"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.023" />

    <ImageView
        android:id="@+id/Sculptres"
        android:layout_width="125dp"
        android:layout_height="125dp"
        android:clickable="true"
        android:contentDescription="@string/cat2"
        android:focusable="true"
        android:onClick="catDetailView"
        android:src="@drawable/sculpture"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.884"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.158" />

    <ImageView
        android:id="@+id/Posters_image"
        android:layout_width="125dp"
        android:layout_height="125dp"
        android:clickable="true"
        android:contentDescription="@string/cat5"
        android:focusable="true"
        android:onClick="catDetailView"
        android:src="@drawable/poster"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.115"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.843" />

    <ImageView
        android:id="@+id/Drawings_image"
        android:layout_width="125dp"
        android:layout_height="125dp"
        android:clickable="true"
        android:contentDescription="@string/cat3"
        android:focusable="true"
        android:onClick="catDetailView"
        android:src="@drawable/drawing"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.115"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/Prints_image"
        android:layout_width="125dp"
        android:layout_height="125dp"
        android:clickable="true"
        android:contentDescription="@string/cat6"
        android:focusable="true"
        android:onClick="catDetailView"
        android:src="@drawable/print"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.884"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.843" />

    <ImageView
        android:id="@+id/Photographs_image"
        android:layout_width="125dp"
        android:layout_height="125dp"
        android:contentDescription="@string/cat4"
        android:onClick="catDetailView"
        android:src="@drawable/photography"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.884"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/Paintings_image"
        android:layout_width="125dp"
        android:layout_height="125dp"
        android:clickable="true"
        android:contentDescription="@string/cat1"
        android:focusable="true"
        android:onClick="catDetailView"
        android:src="@drawable/painting"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.115"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.158" />

    <TextView
        android:id="@+id/painting_txt"
        android:layout_width="90dp"
        android:layout_height="29dp"
        android:layout_marginStart="48dp"
        android:layout_marginLeft="48dp"
        android:clickable="true"
        android:focusable="true"
        android:gravity="center"
        android:onClick="catDetailView"
        android:text="@string/cat1"
        android:textColor="@color/purple"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@id/Drawings_image"
        app:layout_constraintHorizontal_chainStyle="spread"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/Paintings_image"
        app:layout_constraintVertical_bias="0.415" />

    <TextView
        android:id="@+id/sculpture_txt"
        android:layout_width="107dp"
        android:layout_height="26dp"
        android:layout_marginEnd="40dp"
        android:layout_marginRight="40dp"
        android:clickable="true"
        android:focusable="true"
        android:gravity="center"
        android:onClick="catDetailView"
        android:text="@string/cat2"
        android:textColor="@color/purple"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@id/Photographs_image"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.74"
        app:layout_constraintStart_toEndOf="@+id/textView2"
        app:layout_constraintTop_toBottomOf="@id/Sculptres"
        app:layout_constraintVertical_bias="0.446" />

    <TextView
        android:id="@+id/drawing_txt"
        android:layout_width="76dp"
        android:layout_height="29dp"
        android:layout_marginStart="56dp"
        android:layout_marginLeft="56dp"
        android:clickable="true"
        android:focusable="true"
        android:gravity="center"
        android:onClick="catDetailView"
        android:text="@string/cat3"
        android:textColor="@color/purple"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@id/Posters_image"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/Drawings_image"
        app:layout_constraintVertical_bias="0.191" />

    <TextView
        android:id="@+id/photography_txt"
        android:layout_width="108dp"
        android:layout_height="29dp"
        android:clickable="true"
        android:focusable="true"
        android:gravity="center"
        android:onClick="catDetailView"
        android:text="@string/cat4"
        android:textColor="@color/purple"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@id/Posters_image"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.748"
        app:layout_constraintStart_toEndOf="@+id/drawing_txt"
        app:layout_constraintTop_toBottomOf="@id/Photographs_image"
        app:layout_constraintVertical_bias="0.191" />

    <TextView
        android:id="@+id/print_txt"
        android:layout_width="71dp"
        android:layout_height="25dp"
        android:clickable="true"
        android:focusable="true"
        android:gravity="center"
        android:onClick="catDetailView"
        android:text="@string/cat6"
        android:textColor="@color/purple"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.688"
        app:layout_constraintStart_toEndOf="@+id/poster_txt"
        app:layout_constraintTop_toBottomOf="@id/Prints_image"
        app:layout_constraintVertical_bias="0.265" />

    <TextView
        android:id="@+id/poster_txt"
        android:layout_width="69dp"
        android:layout_height="27dp"
        android:layout_marginStart="56dp"
        android:layout_marginLeft="56dp"
        android:clickable="true"
        android:focusable="true"
        android:gravity="center"
        android:onClick="catDetailView"
        android:text="@string/cat5"
        android:textColor="@color/purple"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/Posters_image"
        app:layout_constraintVertical_bias="0.276" />

</androidx.constraintlayout.widget.ConstraintLayout>